{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "baseUrl": ".",//设置baseurl
    "strictNullChecks": true,
    "jsx": "react-jsx",
    "jsxImportSource": "preact",
     "plugins": [
      {
        "name": "@astrojs/ts-plugin"
      }
    ],
     "paths": {
      "@components/*": ["src/components/*"],
      "@assets/*": ["src/assets/*"],
      "@utils/*":["src/utils/*"],
       "@/*": ["src/*"]
    }
  },
  "include": ["src/**/*"]
}